    "version": "0.2.0",
    "configurations": [
        
            {
                "name": "Launch via NPM",
                "type": "chrome",
                "request": "attach",
                "skipFiles": [
                    "<node_internals>/**"
                ],
                "url": "https://localhost:3000",
                "preLaunchTask": "npm: start",
                "sourceMaps": true,
                "sourceMapPathOverrides": {
                    "webpack:///src/*": "${webRoot}/*"
                }
            }
        ]
    }